{
  "name": "officebooking-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:server": "npm --workspace packages/server run dev",
    "dev:web": "npm --workspace packages/web run dev",
    "dev:forecast": "uvicorn packages/forecast/app:app --reload --port 8000"
  }
}
